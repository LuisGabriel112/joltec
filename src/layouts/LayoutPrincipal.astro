---
const { title } = Astro.props;
import "@fontsource/inter";
import "material-symbols/outlined.css";
import "../styles/global.css";
import AOSInitializer from "../components/react/AOSInitializer.jsx";
import Navbar from "../components/common/Navbar.astro";
import LogoJoltec from "../images/logoJoltec.png";
import "animate.css";
---

<!doctype html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href={LogoJoltec.src} />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body class="bg-[#000000]">
    <Navbar />
    <AOSInitializer client:load>
      <slot />
      );
    </AOSInitializer>
  </body>
</html>
